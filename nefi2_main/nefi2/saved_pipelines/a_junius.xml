<?xml version="1.0" encoding="UTF-8"?>
<!-- Settings for a_junius.jpg -->
<config>
    <pipeline>
        <step name="Preprocessing">
          <alg name="Median Blur">
            <param name="ksize">5</param>
          </alg>
        </step>
        <step name="Segmentation">
          <alg name="Guided Watershed with adaptive threshold">
            <param name="bg_iter">0</param>
            <param name="block_size">23</param>
            <param name="constant">1</param>
            <param name="fg_iter">0</param>
          </alg>
        </step>
        <step name="Graph detection">
          <alg name="Guo Hall graph detector"/>
        </step>
        <step name="Graph filtering">
          <alg name="Keep only largest connected component"/>
        </step>
        <step name="Graph filtering">
          <alg name="Simple cycle filter"/>
        </step>
    </pipeline>
</config>
